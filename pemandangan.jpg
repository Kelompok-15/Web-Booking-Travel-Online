<script type="text/javascript">
$(document).ready(function() {
	var bannerCookie = checkCookieS("SESSION_COOKIES");
    if(bannerCookie == true){
		$(".close-cookies-banner").parents(".full-width").remove();
	} else if(bannerCookie == false){
		$(".close-cookies-banner").parents(".full-width").slideDown(300);
	}
	
	var bannerInfoNotice = checkCookieS("SESSION_INFO");
	if(bannerInfoNotice == true){
		$('.more-info').remove();
	} else if(bannerInfoNotice == false){
		$('.more-info').slideDown(250);
	}
});
function removeBannerInfo(){
	$('.more-info').remove();
	setCookiesS(7, "SESSION_INFO");
}
function removeBannerCookiesPolicy() {
    $(".close-cookies-banner").parents(".full-width").slideUp(300);
    setCookiesS(7, "SESSION_COOKIES");
}

function setCookiesS(exdays, name) {
    var d = new Date();
    d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
    var expires = "expires=" + d.toUTCString();
    document.cookie = name+"=agree;" + expires + ";path=/";
}

function deleteCookieSS(name) {
    document.cookie = name + '=;expires=Thu, 01 Jan 1970 00:00:01 GMT;';
};

function get_cookieS(cname) {
    var name = cname + "=";
    var ca = document.cookie.split(';');
    for (var i = 0; i < ca.length; i++) {
        var c = ca[i];
        while (c.charAt(0) == ' ') {
            c = c.substring(1);
        }
        if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
        }
    }
}

function checkCookieS(name) {
    if (get_cookieS(name) === "agree") {
        return true;
    } else {
        return false;
    }
}
</script>

